# board-config-info.shh
#!/bin/bash

CUR_PATH=`pwd`

#
# for user dev board define - rk3288-dev-borad
#

# RF_SYSTEM_TYPE: debian , ubuntu
export RF_SYSTEM_TYPE=debian
# RF_SYSTEM_VERSION: stretch debian9 , buster debian10
export RF_SYSTEM_VERSION=buster
# RF_SOURCE_ROOTFS_PATH: ./debian_rootfs/
export RF_SOURCE_ROOTFS_PATH="debian_buster_rootfs"
# RF_ARCH: arm64 , armhf
export RF_ARCH=armhf
# RF_KERNEL_MODULE_PATH: /xxx/xx/x
export RF_KERNEL_MODULE_PATH=../kernel-module/lib/
# RF_WLAN_KERNEL_MODULE_PATH: /xxx/xx/x
export RF_WLAN_KERNEL_MODULE_PATH=../kernel/drivers/net/wireless/rockchip_wlan